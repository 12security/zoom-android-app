<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:id="@+id/zm_starred_list_item_suggested_linear" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="15dp" android:layout_marginTop="16dp" android:layout_marginBottom="10dp" android:text="@string/zm_mm_starred_channel_suggested_owp40"/>
    </LinearLayout>
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="60dp">
        <FrameLayout android:layout_width="51dp" android:layout_height="51dp" android:layout_marginLeft="15dp">
            <com.zipow.videobox.view.AvatarView android:id="@+id/zm_starred_list_item_avatarView" android:layout_width="45dp" android:layout_height="45dp"/>
            <com.zipow.videobox.view.PresenceStateView android:layout_gravity="right" android:id="@+id/presenceStateView" android:layout_width="wrap_content" android:layout_height="wrap_content" app:zm_subpresence_type="0"/>
        </FrameLayout>
        <TextView android:id="@+id/zm_starred_list_item_Name" android:layout_width="0px" android:layout_height="wrap_content" android:layout_marginLeft="15dp" android:singleLine="true" android:layout_weight="1"/>
        <LinearLayout android:gravity="right|center_vertical|center_horizontal|center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dp">
            <TextView android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/txtAt" android:visibility="gone" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="5dp" android:maxLines="1" style="@style/UIKitTextView.SecondaryText.Small.warn"/>
            <View android:gravity="center" android:id="@+id/unreadBubble" android:background="@drawable/zm_ic_red_dot" android:visibility="gone" android:duplicateParentState="true" android:layout_width="10dp" android:layout_height="10dp" android:layout_marginRight="5dp" android:text=""/>
            <ImageView android:id="@+id/imgErrorMessage" android:visibility="gone" android:layout_width="16dp" android:layout_height="16dp" android:src="@drawable/zm_error_message" android:contentDescription="@string/zm_msg_delivery_error_70196"/>
            <TextView android:gravity="center" android:id="@+id/txtNoteBubble" android:visibility="visible" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/ZMTextView.UnreadMessageCount.Small"/>
            <ImageView android:id="@+id/zm_starred_list_item_star_btn" android:padding="9dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:src="@drawable/zm_mm_starred_icon_on"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
