        <TextView android:id="@+id/slash_command_item_dec" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="3dp" android:text="post a random gif" style="@style/UIKitTextView.SecondaryText.Medium.Dimmed"/>
