        <com.zipow.videobox.view.AvatarView android:id="@+id/slash_command_item_owner_avatar" android:layout_width="16dp" android:layout_height="16dp"/>
        <TextView android:textSize="12sp" android:textColor="@color/zm_ui_kit_color_gray_747487" android:id="@+id/slash_command_item_owner" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="8dp" android:text="Giphy" android:layout_marginStart="8dp"/>
