<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:background="@color/zm_white" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:id="@+id/panelTitleBar" android:background="@drawable/zm_mm_addrbook_bg" android:paddingLeft="10dp" android:paddingRight="10dp" android:layout_width="match_parent" android:layout_height="60dp">
            <Button android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/btnBack" android:layout_width="40dp" android:layout_height="wrap_content" android:text="" android:contentDescription="@string/zm_btn_back" style="@style/ZMBackButton.OnDark"/>
            <LinearLayout android:layout_gravity="center" android:orientation="vertical" android:paddingLeft="8dp" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <p021us.zoom.androidlib.widget.ZMEllipsisTextView android:id="@+id/txtScreenName" android:layout_width="0px" android:layout_height="wrap_content" android:singleLine="true" android:layout_weight="1" style="@style/ZMTextView.Title.OnDark"/>
                    <ImageView android:id="@+id/robotIcon" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="4dp" android:src="@drawable/zm_mm_adddrbook_robot_title_icon" android:contentDescription="@string/zm_accessibility_robot_icon_75690"/>
                </LinearLayout>
                <TextView android:textSize="13sp" android:textColor="@android:color/white" android:id="@+id/txtScreenSubName" android:paddingLeft="5dp" android:paddingRight="5dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="-3dp" android:singleLine="true"/>
            </LinearLayout>
            <ImageView android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/zm_mm_addr_book_detail_starred" android:padding="10dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/zm_mm_starred_title_bar_icon_normal"/>
            <ImageView android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/btnMoreOpts" android:padding="5dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/zm_ic_more" android:contentDescription="@string/zm_description_mm_btn_more"/>
        </LinearLayout>
        <androidx.core.widget.NestedScrollView android:scrollbars="none" android:layout_width="match_parent" android:layout_height="match_parent">
            <LinearLayout android:gravity="top|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/panelUserInfo" android:paddingTop="40dp" android:paddingBottom="26dp" android:layout_width="match_parent" android:layout_height="wrap_content">
                <com.zipow.videobox.view.AvatarView android:id="@+id/avatarView" android:layout_width="104dp" android:layout_height="104dp"/>
                <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/panel_presence" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="16dp">
                    <com.zipow.videobox.view.PresenceStateView android:id="@+id/img_presence" android:focusable="false" android:clickable="false" android:duplicateParentState="true" android:layout_width="12dp" android:layout_height="12dp"/>
                    <TextView android:textSize="@dimen/zm_ui_kit_text_size_15sp" android:textColor="@color/zm_addrbook_item_details_black_text_color" android:id="@+id/txt_presence" android:focusable="false" android:clickable="false" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="8dp"/>
                </LinearLayout>
                <TextView android:textSize="@dimen/zm_ui_kit_text_size_15sp" android:gravity="center" android:id="@+id/txtCustomStatus" android:visibility="visible" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="16dp" android:layout_marginRight="20dp" style="@style/ZMTextView.Normal.Grey"/>
                <androidx.recyclerview.widget.RecyclerView android:id="@+id/zm_mm_addr_book_detail_action_list" android:paddingLeft="32dp" android:paddingRight="32dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="23dp"/>
                <View android:background="@color/zm_line_divider" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="32dp" android:layout_marginTop="24dp" android:layout_marginRight="32dp" android:layout_marginBottom="9dp"/>
                <LinearLayout android:orientation="horizontal" android:id="@+id/pannel_department" android:paddingLeft="32dp" android:paddingTop="24dp" android:paddingRight="32dp" android:focusable="true" android:visibility="gone" android:clickable="true" android:layout_width="match_parent" android:layout_height="wrap_content">
                    <TextView android:focusable="false" android:clickable="false" android:duplicateParentState="true" android:layout_width="@dimen/zm_addrbook_item_details_item_label_width" android:layout_height="wrap_content" android:layout_marginRight="@dimen/zm_addrbook_item_details_item_margin_size" android:text="@string/zm_lbl_profile_department_109011" style="@style/ZMTextView.AddrBookDetails.Gray"/>
                    <TextView android:id="@+id/txt_department" android:focusable="false" android:clickable="false" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/ZMTextView.AddrBookDetails.Black"/>
                </LinearLayout>
                <LinearLayout android:orientation="horizontal" android:id="@+id/panel_job_title" android:paddingLeft="32dp" android:paddingTop="24dp" android:paddingRight="32dp" android:focusable="true" android:visibility="gone" android:clickable="true" android:layout_width="match_parent" android:layout_height="wrap_content">
                    <TextView android:focusable="false" android:clickable="false" android:duplicateParentState="true" android:layout_width="@dimen/zm_addrbook_item_details_item_label_width" android:layout_height="wrap_content" android:layout_marginRight="@dimen/zm_addrbook_item_details_item_margin_size" android:text="@string/zm_lbl_profile_job_title_109011" style="@style/ZMTextView.AddrBookDetails.Gray"/>
                    <TextView android:id="@+id/txt_job_title" android:focusable="false" android:clickable="false" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/ZMTextView.AddrBookDetails.Black"/>
                </LinearLayout>
                <LinearLayout android:orientation="horizontal" android:id="@+id/panel_location" android:paddingLeft="32dp" android:paddingTop="24dp" android:paddingRight="32dp" android:focusable="true" android:visibility="gone" android:clickable="true" android:layout_width="match_parent" android:layout_height="wrap_content">
                    <TextView android:focusable="false" android:clickable="false" android:duplicateParentState="true" android:layout_width="@dimen/zm_addrbook_item_details_item_label_width" android:layout_height="wrap_content" android:layout_marginRight="@dimen/zm_addrbook_item_details_item_margin_size" android:text="@string/zm_lbl_profile_location_109011" style="@style/ZMTextView.AddrBookDetails.Gray"/>
                    <TextView android:id="@+id/txt_location" android:focusable="false" android:clickable="false" android:duplicateParentState="true" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/ZMTextView.AddrBookDetails.Black"/>
                </LinearLayout>
                <View android:id="@+id/line_divider" android:background="@color/zm_line_divider" android:visibility="gone" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="32dp" android:layout_marginTop="27dp" android:layout_marginRight="32dp" android:layout_marginBottom="5dp"/>
                <androidx.recyclerview.widget.RecyclerView android:id="@+id/detailRecyclerView" android:layout_width="match_parent" android:layout_height="wrap_content"/>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
    <ViewStub android:id="@+id/tipsViewStub" android:layout="@layout/zm_mm_im_tips" android:inflatedId="@+id/tipsInflate" android:layout_width="match_parent" android:layout_height="match_parent"/>
</FrameLayout>
