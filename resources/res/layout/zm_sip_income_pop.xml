        <TextView android:textSize="@dimen/zm_ui_kit_text_size_16sp" android:textColor="@color/zm_ui_kit_color_black_232333" android:gravity="center" android:id="@+id/tvCallingForNumber" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:lineSpacingExtra="2sp"/>
    </LinearLayout>
    <LinearLayout android:gravity="center_horizontal" android:orientation="horizontal" android:id="@+id/panelCallBtns" android:paddingLeft="20dp" android:paddingRight="20dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="32dp" android:layout_marginBottom="20dp" android:scaleX="0.83" android:scaleY="0.83">
        <LinearLayout android:gravity="top|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/panelEndAcceptCall" android:visibility="gone" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
            <ImageView android:id="@+id/btnEndAcceptCall" android:layout_width="@dimen/zm_sip_dialpad_key_width" android:layout_height="@dimen/zm_sip_dialpad_key_width" android:src="@drawable/zm_sip_end_accept" android:scaleType="fitXY" android:contentDescription="@string/zm_sip_end_accept_61381"/>
            <TextView android:gravity="center" android:id="@+id/txtEndAcceptCall" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:text="@string/zm_sip_end_accept_61381" android:importantForAccessibility="no" style="@style/ZMTextView.Normal.Grey"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:id="@+id/panelEndCall" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
            <ImageView android:id="@+id/btnEndCall" android:duplicateParentState="true" android:layout_width="@dimen/zm_sip_dialpad_key_width" android:layout_height="@dimen/zm_sip_dialpad_key_width" android:src="@drawable/zm_sip_end_call" android:scaleType="fitXY" android:contentDescription="@string/zm_sip_btn_decline_61431"/>
            <TextView android:id="@+id/txtEndCall" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:text="@string/zm_sip_btn_decline_61431" android:importantForAccessibility="no" style="@style/ZMTextView.Normal.OnLight"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:id="@+id/panelAcceptCall" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
            <ImageView android:id="@+id/btnAcceptCall" android:duplicateParentState="true" android:layout_width="@dimen/zm_sip_dialpad_key_width" android:layout_height="@dimen/zm_sip_dialpad_key_width" android:src="@drawable/zm_sip_start_call_nodisable" android:scaleType="fitXY" android:contentDescription="@string/zm_btn_accept_call_14480"/>
            <TextView android:id="@+id/txtAccpetCall" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:text="@string/zm_btn_accept_sip_61381" android:importantForAccessibility="no" style="@style/ZMTextView.Normal.OnLight"/>
        </LinearLayout>
