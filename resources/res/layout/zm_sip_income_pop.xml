    <TextView android:gravity="center_horizontal" android:id="@+id/tvStatus" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="5dp" android:layout_marginRight="20dp" android:text="@string/zm_sip_income_status_text_26673" android:maxLines="1" android:layout_below="@+id/tvBuddyName" android:scaleX="0.83" android:scaleY="0.83" style="@style/ZMTextView.BuddyName.Normal.OnLight"/>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/panelCallType" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="20dp" android:scaleX="0.83" android:scaleY="0.83">
        <TextView android:textSize="@dimen/zm_ui_kit_text_size_11sp" android:textStyle="bold" android:textColor="@color/zm_ui_kit_color_gray_747487" android:id="@+id/tvCallingForTitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="FOR"/>
