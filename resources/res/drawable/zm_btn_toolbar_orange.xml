<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_toolbar_radius"/>
            <gradient android:startColor="@color/zm_ui_kit_color_yellow_FF742E_press" android:endColor="@color/zm_ui_kit_color_yellow_FF742E_press" android:angle="0"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_toolbar_radius"/>
            <gradient android:startColor="@color/zm_ui_kit_color_yellow_FF742E" android:endColor="@color/zm_ui_kit_color_yellow_FF742E" android:angle="0"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_toolbar_radius"/>
            <gradient android:startColor="@color/zm_ui_kit_color_yellow_FF742E_disable" android:endColor="@color/zm_ui_kit_color_yellow_FF742E_disable" android:angle="0"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_toolbar_radius"/>
            <gradient android:startColor="@color/zm_ui_kit_color_yellow_FF742E" android:endColor="@color/zm_ui_kit_color_yellow_FF742E" android:angle="0"/>
        </shape>
    </item>
</selector>
